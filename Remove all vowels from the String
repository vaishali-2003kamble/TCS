
class Main {
    public static void main(String[] args) {
       String s="I am very happy today";
       String res=" ";
       for(int i=0;i<s.length()-1;i++)
       {
           char ch=s.charAt(i);
           if(ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u' 
           || ch=='A' || ch=='E' || ch=='I' || ch=='O' || ch=='U')
           {
               continue;
           }
           res=res+ch;
       }
       System.out.println("after removing all the vowels ;- "+res);
    }
}
