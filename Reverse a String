
class Main {
    public static void main(String[] args) {
       String s="OLLEH";
       char arr[]=s.toCharArray();
       int left=0;
       int right=s.length()-1;
       while(left<right)
       {
          char temp=arr[left];
           arr[left]=arr[right];
           arr[right]=temp;
           left++;
           right--;
       }
       
       System.out.println("Revser string is :- "+new String(arr));
    }
}
