
class Main {
    public static void main(String[] args) {
       String s="madam";
       char arr[]=s.toCharArray();
       int left=0;
       int right=s.length()-1;
       while(left<right)
       {
          char temp=arr[left];
          arr[left]=arr[right];
          arr[right]=temp;
           left++;
           right--;
       }
  
       String reverse=new String(arr);
       if(s.equals(reverse))
       {
    	   System.out.println("is palindrome");
       }
       else
       {
    	   System.out.println("Not palindrome");
       }
       
    }
}
