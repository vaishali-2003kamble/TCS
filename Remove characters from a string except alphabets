
class Main {
    public static void main(String[] args) {
       String s="take12% *&u ^$#forward";
       String res=" ";
       for(int i=0;i<s.length();i++)
       {
           char ch=s.charAt(i);
           if(ch>='a' && ch<='z' )
           {
              res=res+ch;
           }
           
       }
       System.out.println("after removing all the  special symbol ;- "+res);
    }
}
