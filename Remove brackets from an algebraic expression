
class Main {
    public static void main(String[] args) {
       String s="(((a-b))+c)";
       String res=" ";
       for(int i=0;i<s.length();i++)
       {
           char ch=s.charAt(i);
           if(ch=='(' || ch==')')
           {
               continue;
           }
           res=res+ch;
       }
       System.out.println("Remove bracket "+res);
      
    }
}
