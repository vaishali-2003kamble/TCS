import java.util.*;
class Main {
    public static void main(String[] args) {
      int arr[]={1,2,1,1,5,1};
      int n=arr.length;
      double sum=0.0;
      double avg=0.0;
     for(int i=0;i<n;i++)
     {
         sum=sum+arr[i];
     }
     avg=sum/n;
     System.out.println("Avg of array "+(avg));
    }
}
