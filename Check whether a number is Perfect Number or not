
class Main {
    public static void main(String[] args) {
      int num=15;
      int copy=num;
     int sum=0;
     for(int i=1;i<=num/2;i++)
     {
         if(num%i==0)
         {
             sum=sum+i;
         }
     }
     if(sum==copy)
     {
         System.out.println("Perfect square "+sum);
     }
     else{
         System.out.println("not perfect suqare "+sum);
     }

    }
        
}
