
class Main {
    public static void main(String[] args) {
       String s="I am very happy today";
       String res=" ";
       for(int i=0;i<s.length();i++)
       {
           char ch=s.charAt(i);
           if(ch==' ' )
           {
               continue;
           }
           res=res+ch;
       }
       System.out.println("after removing all the vowels ;- "+res);
    }
}
